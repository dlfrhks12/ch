/* 음식점  */
CREATE TABLE store (
	s_num NVARCHAR2(50) NOT NULL, /* 음식점 고유번호 */
	s_review CLOB, /* 해당매장의 리뷰 */
	s_sajin VARCHAR2(100) NOT NULL, /* 매장사진 */
	s_info CLOB NOT NULL, /* 매장정보 */
	s_name NVARCHAR2(20) NOT NULL, /* 상호명 */
	s_tel VARCHAR2(11) NOT NULL, /* 음식점 전화번호 */
	s_address NVARCHAR2(100), /* 주소 */
	food_no NUMBER(10), /* 음식 카테고리 번호 */
	s_star_point NUMBER(10), /* 별점평균 */
	s_review_cnt NUMBER(10) /* 리뷰개수 */
);

COMMENT ON TABLE store IS '음식점 ';

COMMENT ON COLUMN store.s_num IS '음식점 고유번호';

COMMENT ON COLUMN store.s_review IS '해당매장의 리뷰';

COMMENT ON COLUMN store.s_sajin IS '매장사진';

COMMENT ON COLUMN store.s_info IS '매장정보';

COMMENT ON COLUMN store.s_name IS '상호명';

COMMENT ON COLUMN store.s_tel IS '음식점 전화번호';

COMMENT ON COLUMN store.s_address IS '주소';

COMMENT ON COLUMN store.food_no IS '음식 카테고리 번호';

COMMENT ON COLUMN store.s_star_point IS '별점평균';

COMMENT ON COLUMN store.s_review_cnt IS '리뷰개수';

CREATE UNIQUE INDEX PK_store
	ON store (
		s_num ASC
	);

/* 음식 카테고리 */
CREATE TABLE foodcategory (
	food_no NUMBER(10) NOT NULL, /* 음식 카테고리 번호 */
	food_category NVARCHAR2(10) /* 음식카테고리명 */
);

COMMENT ON TABLE foodcategory IS '음식 카테고리';

COMMENT ON COLUMN foodcategory.food_no IS '음식 카테고리 번호';

COMMENT ON COLUMN foodcategory.food_category IS '음식카테고리명';

CREATE UNIQUE INDEX PK_foodcategory
	ON foodcategory (
		food_no ASC
	);

/* 문의 답변 카테고리 */
CREATE TABLE qnacategory (
	q_cano NUMBER(10) NOT NULL, /* 문의카테고리 번호 */
	q_category NVARCHAR2(60) NOT NULL /* 문의카테고리명 */
);

COMMENT ON TABLE qnacategory IS '문의 답변 카테고리';

COMMENT ON COLUMN qnacategory.q_cano IS '문의카테고리 번호';

COMMENT ON COLUMN qnacategory.q_category IS '문의카테고리명';

CREATE UNIQUE INDEX PK_qnacategory
	ON qnacategory (
		q_cano ASC
	);

/* 문의답변 */
CREATE TABLE qna (
	q_no NUMBER(10) NOT NULL, /* 문의 글번호 */
	q_username VARCHAR2(20) NOT NULL, /* 아이디 */
	q_title NVARCHAR2(50) NOT NULL, /* 제목 */
	q_content CLOB NOT NULL, /* 내용 */
	q_write_time DATE NOT NULL, /* 작성시간 */
	q_iscomment NUMBER(1), /* 답변유무 */
	m_num NVARCHAR2(50), /* 사업자등록번호 */
	q_cano NUMBER(10) /* 문의카테고리 번호 */
);

COMMENT ON TABLE qna IS '문의답변';

COMMENT ON COLUMN qna.q_no IS '문의 글번호';

COMMENT ON COLUMN qna.q_username IS '아이디';

COMMENT ON COLUMN qna.q_title IS '제목';

COMMENT ON COLUMN qna.q_content IS '내용';

COMMENT ON COLUMN qna.q_write_time IS '작성시간';

COMMENT ON COLUMN qna.q_iscomment IS '답변유무';

COMMENT ON COLUMN qna.m_num IS '사업자등록번호';

COMMENT ON COLUMN qna.q_cano IS '문의카테고리 번호';

CREATE UNIQUE INDEX PK_qna
	ON qna (
		q_no ASC
	);

/* 공지사항 */
CREATE TABLE notice (
	n_no NUMBER(10) NOT NULL, /* 공지사항 글번호 */
	n_title NVARCHAR2(50) NOT NULL, /* 제목 */
	n_write_time DATE NOT NULL, /* 작성일 */
	n_readcnt NUMBER(7) NOT NULL, /* 조회수 */
	content CLOB NOT NULL, /* 내용 */
	a_username VARCHAR2(20) /* 관리자 아이디 */
);

COMMENT ON TABLE notice IS '공지사항';

COMMENT ON COLUMN notice.n_no IS '공지사항 글번호';

COMMENT ON COLUMN notice.n_title IS '제목';

COMMENT ON COLUMN notice.n_write_time IS '작성일';

COMMENT ON COLUMN notice.n_readcnt IS '조회수';

COMMENT ON COLUMN notice.content IS '내용';

COMMENT ON COLUMN notice.a_username IS '관리자 아이디';

CREATE UNIQUE INDEX PK_notice
	ON notice (
		n_no ASC
	);

/* 고지서 */
CREATE TABLE paybill (
	m_num NVARCHAR2(50) NOT NULL, /* 사업자등록번호 */
	p_no NUMBER(10) NOT NULL, /* 고지서번호 */
	p_msal NUMBER(10) NOT NULL, /* 월매출액 */
	p_sal NUMBER(10) NOT NULL, /* 납부금액 */
	p_day DATE NOT NULL, /* 납부일 */
	p_check NUMBER(1) /* 납입여부 */
);

COMMENT ON TABLE paybill IS '고지서';

COMMENT ON COLUMN paybill.m_num IS '사업자등록번호';

COMMENT ON COLUMN paybill.p_no IS '고지서번호';

COMMENT ON COLUMN paybill.p_msal IS '월매출액';

COMMENT ON COLUMN paybill.p_sal IS '납부금액';

COMMENT ON COLUMN paybill.p_day IS '납부일';

COMMENT ON COLUMN paybill.p_check IS '납입여부';

CREATE UNIQUE INDEX PK_paybill
	ON paybill (
		m_num ASC,
		p_no ASC
	);

/* 리뷰 */
CREATE TABLE review (
	r_no NUMBER(10) NOT NULL, /* 리뷰번호 */
	r_content CLOB NOT NULL, /* 내용 */
	r_star_point NUMBER(10) NOT NULL, /* 별점 */
	r_write_time DATE NOT NULL, /* 작성시간 */
	o_no NUMBER(10) NOT NULL, /* 주문번호 */
	r_title NVARCHAR2(50) NOT NULL, /* 제목 */
	r_report NUMBER(10), /* 신고수 */
	u_username VARCHAR2(20), /* 아이디 */
	s_num NVARCHAR2(50), /* 음식점 고유번호 */
	rc_no NUMBER(10) /* 리뷰답글번호 */
);

COMMENT ON TABLE review IS '리뷰';

COMMENT ON COLUMN review.r_no IS '리뷰번호';

COMMENT ON COLUMN review.r_content IS '내용';

COMMENT ON COLUMN review.r_star_point IS '별점';

COMMENT ON COLUMN review.r_write_time IS '작성시간';

COMMENT ON COLUMN review.o_no IS '주문번호';

COMMENT ON COLUMN review.r_title IS '제목';

COMMENT ON COLUMN review.r_report IS '신고수';

COMMENT ON COLUMN review.u_username IS '아이디';

COMMENT ON COLUMN review.s_num IS '음식점 고유번호';

COMMENT ON COLUMN review.rc_no IS '리뷰답글번호';

CREATE UNIQUE INDEX PK_review
	ON review (
		r_no ASC
	);

