/* 첨부파일 */
CREATE TABLE attachment (
	f_no NUMBER(10) NOT NULL, /* 파일 번호 */
	f_writer NVARCHAR2(20) NOT NULL, /* 작성자 */
	original_file_name VARCHAR2(100) NOT NULL, /* 원본 파일 이름 */
	save_file_name VARCHAR2(120) NOT NULL, /* 저장 파일 이름 */
	f_length NUMBER(5) NOT NULL, /* 길이 */
	b_no NUMBER(10) NOT NULL, /* 글번호 */
	is_image NUMBER(1) /* 이미지 여부 */
);

COMMENT ON TABLE attachment IS '첨부파일';

COMMENT ON COLUMN attachment.f_no IS '파일 번호';

COMMENT ON COLUMN attachment.f_writer IS '작성자';

COMMENT ON COLUMN attachment.original_file_name IS '원본 파일 이름';

COMMENT ON COLUMN attachment.save_file_name IS '저장 파일 이름';

COMMENT ON COLUMN attachment.f_length IS '길이';

COMMENT ON COLUMN attachment.b_no IS '글번호';

COMMENT ON COLUMN attachment.is_image IS '이미지 여부';

CREATE UNIQUE INDEX PK_attachment
	ON attachment (
		f_no ASC
	);

