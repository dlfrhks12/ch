/* 관리자 */
CREATE TABLE admin (
	a_username VARCHAR2(20) NOT NULL, /* 관리자 아이디 */
	a_password VARCHAR2(60) NOT NULL, /* 비밀번호 */
	a_irum NVARCHAR2(10) NOT NULL, /* 이름 */
	a_enabled NUMBER(1) /* 로그인 여부 */
);

COMMENT ON TABLE admin IS '관리자';

COMMENT ON COLUMN admin.a_username IS '관리자 아이디';

COMMENT ON COLUMN admin.a_password IS '비밀번호';

COMMENT ON COLUMN admin.a_irum IS '이름';

COMMENT ON COLUMN admin.a_enabled IS '로그인 여부';

CREATE UNIQUE INDEX PK_admin
	ON admin (
		a_username ASC
	);

/* 사업자회원 */
CREATE TABLE manager (
	m_num NVARCHAR2(50) NOT NULL, /* 사업자등록번호 */
	m_password VARCHAR2(60) NOT NULL, /* 비밀번호 */
	m_username VARCHAR2(20) NOT NULL, /* 아이디 */
	m_email VARCHAR2(30) NOT NULL, /* 이메일 */
	m_tel VARCHAR2(11) NOT NULL, /* 사업자 전화번호 */
	m_account VARCHAR2(20) NOT NULL, /* 전용계좌번호 */
	m_irum NVARCHAR2(10) NOT NULL, /* 사업자 이름 */
	s_name NVARCHAR2(20) NOT NULL /* 상호명 */
);

COMMENT ON TABLE manager IS '사업자회원';

COMMENT ON COLUMN manager.m_num IS '사업자등록번호';

COMMENT ON COLUMN manager.m_password IS '비밀번호';

COMMENT ON COLUMN manager.m_username IS '아이디';

COMMENT ON COLUMN manager.m_email IS '이메일';

COMMENT ON COLUMN manager.m_tel IS '사업자 전화번호';

COMMENT ON COLUMN manager.m_account IS '전용계좌번호';

COMMENT ON COLUMN manager.m_irum IS '사업자 이름';

COMMENT ON COLUMN manager.s_name IS '상호명';

CREATE UNIQUE INDEX PK_manager
	ON manager (
		m_num ASC
	);

/* 일반회원 */
CREATE TABLE users (
	u_username VARCHAR2(20) NOT NULL, /* 아이디 */
	u_irum NVARCHAR2(10) NOT NULL, /* 이름 */
	u_email VARCHAR2(30) NOT NULL, /* 이메일 */
	u_tel VARCHAR2(11) NOT NULL, /* 전화번호 */
	u_address NVARCHAR2(100) NOT NULL, /* 주소 */
	u_password VARCHAR2(60) NOT NULL, /* 비밀번호 */
	u_enabled NUMBER(1), /* 활성화여부 */
	u_joindate DATE NOT NULL, /* 가입일자 */
	u_point NUMBER(10), /* 포인트 */
	u_login_fail_cnt NUMBER(1) /* 로그인실패횟수 */
);

COMMENT ON TABLE users IS '일반회원';

COMMENT ON COLUMN users.u_username IS '아이디';

COMMENT ON COLUMN users.u_irum IS '이름';

COMMENT ON COLUMN users.u_email IS '이메일';

COMMENT ON COLUMN users.u_tel IS '전화번호';

COMMENT ON COLUMN users.u_address IS '주소';

COMMENT ON COLUMN users.u_password IS '비밀번호';

<<<<<<< HEAD
