/* 자유게시판 */
CREATE TABLE board (
	b_no NUMBER(10) NOT NULL, /* 자유글번호 */
	b_title NVARCHAR2(50) NOT NULL, /* 제목 */
	b_content CLOB NOT NULL, /* 내용 */
	b_write_time DATE NOT NULL, /* 작성시간 */
	b_read_cnt NUMBER(7), /* 조회수 */
	b_good_cnt NUMBER(7), /* 추천수 */
	b_bad_cnt NUMBER(7), /* 비추수 */
	b_category NVARCHAR2(10), /* 카테고리 */
	b_comment_cnt NUMBER(7), /* 댓글수 */
	f_no NUMBER(10), /* 파일 번호 */
	c_no NUMBER(10), /* 댓글번호 */
	u_username VARCHAR2(20) /* 아이디 */
);

COMMENT ON TABLE board IS '자유게시판';

COMMENT ON COLUMN board.b_no IS '자유글번호';

COMMENT ON COLUMN board.b_title IS '제목';

COMMENT ON COLUMN board.b_content IS '내용';

COMMENT ON COLUMN board.b_write_time IS '작성시간';

COMMENT ON COLUMN board.b_read_cnt IS '조회수';

COMMENT ON COLUMN board.b_good_cnt IS '추천수';

COMMENT ON COLUMN board.b_bad_cnt IS '비추수';

COMMENT ON COLUMN board.b_category IS '카테고리';

COMMENT ON COLUMN board.b_comment_cnt IS '댓글수';

COMMENT ON COLUMN board.f_no IS '파일 번호';

COMMENT ON COLUMN board.c_no IS '댓글번호';

COMMENT ON COLUMN board.u_username IS '아이디';

CREATE UNIQUE INDEX PK_board
	ON board (
		b_no ASC
	);

