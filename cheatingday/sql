	menusajin VARCHAR2(100), /* 사진 */
	menuname NVARCHAR2(30) NOT NULL, /* 이름 */
	s_num NVARCHAR2(50) /* 음식점 고유번호 */
);

COMMENT ON TABLE menu IS '메뉴';

COMMENT ON COLUMN menu.menuno IS '메뉴번호';

COMMENT ON COLUMN menu.menusal IS '가격';

COMMENT ON COLUMN menu.menusajin IS '사진';

COMMENT ON COLUMN menu.menuname IS '이름';

COMMENT ON COLUMN menu.s_num IS '음식점 고유번호';

CREATE UNIQUE INDEX PK_menu
	ON menu (
		menuno ASC
	);

/* 주문상세 */
CREATE TABLE detailorder (
	d_no NUMBER(10) NOT NULL, /* 상세번호 */
	d_sal NUMBER(10) NOT NULL, /* 금액 */
	d_menu_cnt NUMBER(10) NOT NULL, /* 수량 */
	d_menu_name NVARCHAR2(30) NOT NULL, /* 이름 */
	o_no NUMBER(10), /* 주문번호 */
	menuno NUMBER(10) /* 메뉴번호 */
);

COMMENT ON TABLE detailorder IS '주문상세';

COMMENT ON COLUMN detailorder.d_no IS '상세번호';

COMMENT ON COLUMN detailorder.d_sal IS '금액';

COMMENT ON COLUMN detailorder.d_menu_cnt IS '수량';

COMMENT ON COLUMN detailorder.d_menu_name IS '이름';

COMMENT ON COLUMN detailorder.o_no IS '주문번호';

COMMENT ON COLUMN detailorder.menuno IS '메뉴번호';

CREATE UNIQUE INDEX PK_detailorder
	ON detailorder (
		d_no ASC
	);

/* 달력 */
CREATE TABLE calender (
	day DATE NOT NULL, /* 날짜 */
	check_open NUMBER(1) /* 영업여부 */
);

COMMENT ON TABLE calender IS '달력';

COMMENT ON COLUMN calender.day IS '날짜';

COMMENT ON COLUMN calender.check_open IS '영업여부';

CREATE UNIQUE INDEX PK_calender
	ON calender (
		day ASC
	);

/* 판매 */
CREATE TABLE sales (
	day DATE NOT NULL, /* 날짜 */
	menuno NUMBER(10) NOT NULL, /* 메뉴번호 */
	s_num NVARCHAR2(50) NOT NULL, /* 음식점 고유번호 */
	sal_count NUMBER(10) NOT NULL /* 매출 */
);

COMMENT ON TABLE sales IS '판매';

COMMENT ON COLUMN sales.day IS '날짜';

COMMENT ON COLUMN sales.menuno IS '메뉴번호';

COMMENT ON COLUMN sales.s_num IS '음식점 고유번호';

COMMENT ON COLUMN sales.sal_count IS '매출';

CREATE UNIQUE INDEX PK_sales
	ON sales (
		day ASC,
		menuno ASC,
		s_num ASC
	);

/* 주소정보 */
CREATE TABLE address (
	ad_no NVARCHAR2(1000) NOT NULL, /* 우편번호 */
	ad_road NVARCHAR2(1000) NOT NULL, /* 도로명주소 */
	ad_extra NVARCHAR2(1000) NOT NULL, /* 참고주소 */
	ad_detail NVARCHAR2(1000) NOT NULL, /* 상세주소 */
	ad_total NVARCHAR2(1000) NOT NULL, /* 전체주소 */
	ad_best NVARCHAR2(1000) NOT NULL, /* 대표주소 */
	u_username VARCHAR2(20) /* 아이디 */
);

COMMENT ON TABLE address IS '주소정보';

COMMENT ON COLUMN address.ad_no IS '우편번호';

COMMENT ON COLUMN address.ad_road IS '도로명주소';

COMMENT ON COLUMN address.ad_extra IS '참고주소';

COMMENT ON COLUMN address.ad_detail IS '상세주소';

COMMENT ON COLUMN address.ad_total IS '전체주소';

COMMENT ON COLUMN address.ad_best IS '대표주소';

COMMENT ON COLUMN address.u_username IS '아이디';

/* 즐겨찾기 */
CREATE TABLE favorites (
	u_username VARCHAR2(20), /* 아이디 */
	s_num NVARCHAR2(50) /* 음식점 고유번호 */
);

COMMENT ON TABLE favorites IS '즐겨찾기';

COMMENT ON COLUMN favorites.u_username IS '아이디';

COMMENT ON COLUMN favorites.s_num IS '음식점 고유번호';

/* 신고리뷰 */
CREATE TABLE TABLE7 (
	a_username VARCHAR2(20), /* 관리자 아이디 */
	r_no NUMBER(10), /* 리뷰번호 */
	u_username VARCHAR2(20), /* 아이디 */
	s_num NVARCHAR2(50) /* 음식점 고유번호 */
);

COMMENT ON TABLE TABLE7 IS '신고리뷰';

COMMENT ON COLUMN TABLE7.a_username IS '관리자 아이디';

COMMENT ON COLUMN TABLE7.r_no IS '리뷰번호';

COMMENT ON COLUMN TABLE7.u_username IS '아이디';

COMMENT ON COLUMN TABLE7.s_num IS '음식점 고유번호';

/* 리뷰답글 */
CREATE TABLE reviewcomment (
	rc_no NUMBER(10) NOT NULL, /* 리뷰답글번호 */
	r_no NUMBER(10), /* 리뷰번호 */
	m_num NVARCHAR2(50), /* 사업자등록번호 */
	rc_content CLOB NOT NULL, /* 답글내용 */
	rc_date_time DATE NOT NULL /* 답글시간 */
);

COMMENT ON TABLE reviewcomment IS '리뷰답글';

COMMENT ON COLUMN reviewcomment.rc_no IS '리뷰답글번호';

COMMENT ON COLUMN reviewcomment.r_no IS '리뷰번호';

COMMENT ON COLUMN reviewcomment.m_num IS '사업자등록번호';

COMMENT ON COLUMN reviewcomment.rc_content IS '답글내용';

COMMENT ON COLUMN reviewcomment.rc_date_time IS '답글시간';

CREATE UNIQUE INDEX PK_reviewcomment
	ON reviewcomment (
		rc_no ASC
	);

/* 문의답변댓글 */
CREATE TABLE qnacomment (
	qc_no NUMBER(10) NOT NULL, /* 문의댓글 글번호 */
	q_no NUMBER(10), /* 문의 글번호 */
	a_username VARCHAR2(20), /* 관리자 아이디 */
	qc_content CLOB NOT NULL, /* 댓글내용 */
	qc_write_time DATE NOT NULL /* 댓글시간 */
);

COMMENT ON TABLE qnacomment IS '문의답변댓글';

COMMENT ON COLUMN qnacomment.qc_no IS '문의댓글 글번호';

COMMENT ON COLUMN qnacomment.q_no IS '문의 글번호';

COMMENT ON COLUMN qnacomment.a_username IS '관리자 아이디';

COMMENT ON COLUMN qnacomment.qc_content IS '댓글내용';

COMMENT ON COLUMN qnacomment.qc_write_time IS '댓글시간';

CREATE UNIQUE INDEX PK_qnacomment
	ON qnacomment (
		qc_no ASC
	);